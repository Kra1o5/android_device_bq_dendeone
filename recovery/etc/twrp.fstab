/cache            ext4       /dev/block/mmcblk0p26
/system           ext4       /dev/block/mmcblk0p25
/system_image     emmc       /dev/block/mmcblk0p25
/data             ext4       /dev/block/mmcblk0p27   length=-16384
/boot             emmc       /dev/block/mmcblk0p7
/recovery         emmc       /dev/block/mmcblk0p8
/external_sd      vfat       /dev/block/mmcblk1p1 /dev/block/mmcblk1  flags=display="External SD";storage;wipeingui;removable
/usb-otg          vfat       /dev/block/sda1 /dev/block/sda           flags=display="USB OTG";storage;wipeingui;removable
